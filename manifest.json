{
  "manifest_version": 2,
  "name": "ShopReal",
  "version": "1.0.1",
  "author": "harrykp",
  "description": "Hide the product images and show the original ones i.e. review images",
  "homepage_url": "https://www.amazon.in/",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "100": "images/icon-100.png"
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "js": ["js/after.js"],
      "matches": ["https://www.amazon.in/*"]
    }
  ]
}
